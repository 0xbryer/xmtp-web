{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Vite",
  "extends": "./base.json",
  "compilerOptions": {
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "module": "ESNext",
    "noEmit": true,
    "resolveJsonModule": true,
    "sourceMap": true,
    "target": "ESNext",
    "types": ["vite/client"],
    "useDefineForClassFields": true
  }
}
