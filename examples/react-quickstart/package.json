{
  "name": "@xmtp/react-quickstart-example",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "yarn typecheck && vite build",
    "clean:dist": "rm -rf dist",
    "clean": "rm -rf .turbo && rm -rf node_modules && yarn clean:dist",
    "lint": "eslint . --ignore-path ../../.gitignore",
    "format:base": "prettier --ignore-path ../../.gitignore",
    "format:check": "yarn format:base -c .",
    "format": "yarn format:base -w .",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@heroicons/react": "^2.0.18",
    "@rainbow-me/rainbowkit": "^0.12.4",
    "@xmtp/react-components": "workspace:*",
    "@xmtp/react-sdk": "workspace:*",
    "ethers": "5.7.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "wagmi": "^0.12.13"
  },
  "devDependencies": {
    "@types/react": "^18.2.12",
    "@types/react-dom": "^18.2.5",
    "@vitejs/plugin-react": "^4.0.0",
    "@xmtp/tsconfig": "workspace:*",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.42.0",
    "eslint-config-xmtp-web": "workspace:*",
    "postcss": "^8.4.24",
    "postcss-preset-env": "^8.5.0",
    "typescript": "^5.1.3",
    "vite": "^4.3.9"
  }
}
