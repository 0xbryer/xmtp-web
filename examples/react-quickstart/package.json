{
  "name": "@xmtp/react-quickstart-example",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "yarn typecheck && vite build",
    "clean:dist": "rm -rf dist",
    "clean": "rm -rf .turbo && rm -rf node_modules && yarn clean:dist",
    "lint": "eslint . --ignore-path ../../.gitignore",
    "format:base": "prettier --ignore-path ../../.gitignore",
    "format:check": "yarn format:base -c .",
    "format": "yarn format:base -w .",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@heroicons/react": "^2.0.16",
    "@rainbow-me/rainbowkit": "^0.12.4",
    "@xmtp/react-sdk": "workspace:*",
    "@xmtp/xmtp-js": "^8.0.1",
    "ethers": "5.7.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "wagmi": "^0.12.6"
  },
  "devDependencies": {
    "@types/react": "^18.0.29",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^3.1.0",
    "@xmtp/tsconfig": "workspace:*",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.36.0",
    "eslint-config-xmtp-web": "workspace:*",
    "postcss": "^8.4.21",
    "typescript": "^5.0.2",
    "vite": "^4.2.1"
  }
